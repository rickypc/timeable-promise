    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@rspack/cli": "1.6.5",
    "@rspack/core": "1.6.5",
    "@swc/core": "1.15.3",
    "@swc/jest": "0.2.39",
    "@types/airbnb__node-memwatch": "2.0.4",
    "@types/eslint-plugin-security": "3.0.0",
    "@types/jest": "30.0.0",
    "bench-node": "0.13.0",
    "eslint": "9.39.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.2.1",
    "eslint-plugin-jsdoc": "61.4.1",
    "eslint-plugin-no-secrets": "2.2.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-yml": "1.19.0",
    "jest": "30.2.0",
    "jiti": "2.6.1",
    "remove-types": "1.0.0",
    "ts-checker-rspack-plugin": "1.2.0",
    "typedoc": "0.28.15",
    "typedoc-plugin-markdown": "4.9.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0",
    "yaml-eslint-parser": "1.3.1"
  },
  "homepage": "https://github.com/rickypc/timeable-promise#readme",
  "imports": {
    "#root/*": "./*"
  },
  "keywords": [
    "promise utilities",
    "concurrency",
    "sequential execution",
    "async control",
    "timeout handling",
    "polling",
    "sleep",
    "task scheduling",
    "typescript",
    "utility library"
  ],
  "license": "AGPL-3.0",
  "name": "timeable-promise",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rickypc/timeable-promise.git"
  },
  "scripts": {
    "build": "yarn build:docs && yarn build:dist",
    "build:dist": "rspack build",
    "build:docs": "typedoc",
    "prepublishOnly": "yarn test && yarn build",
    "publish": "git tag $(npm pkg get version | tr -d '\"') && git push --tags && cd ./dist && npm publish",
    "publish:dry": "yarn prepublishOnly && cd ./dist && npm publish --dry-run && echo \"\\033[1;32mgit commit && git push origin && yarn publish\\033[0m\" || echo \"\\033[1;31m$(npm pkg get name | tr -d '\"') v$(npm pkg get version | tr -d '\"') is published. Do you want to bump up the version and modify CHANGELOG?\\033[0m\"",
    "test": "yarn test:lint && yarn test:type && yarn test:unit && yarn test:perf && yarn test:leak",
    "test:deps:ci": "yarn outdated --json | jq -r 'select(.type == \"table\") | .data.body[] | @tsv' | sed 's/\\t/ | /g'",
    "test:leak": "sh -c 'RESILIENT_TYPE=leak jest \"$@\"' --",
    "test:leak:ci": "CI=true yarn test:leak",
    "test:lint": "eslint .",
    "test:perf": "sh -c 'RESILIENT_TYPE=perf node --allow-natives-syntax ./node_modules/.bin/jest \"$@\"' --",
    "test:perf:ci": "CI=true yarn test:perf",
    "test:type": "tsc",
    "test:unit": "sh -c 'jest \"$@\"' --",
    "test:unit:ci": "CI=true yarn test:unit"
  },
  "type": "module",
  "version": "1.7.1"
}
